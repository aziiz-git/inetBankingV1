<suite name="InetBankingV1" thread-count="3" parallel="tests"
	verbose="3">


	<listeners>
		<listener class-name="com.inetBanking.utilities.Reporting" />
	</listeners>

	<test name="BankingApp Test -- Can be anything">
		<parameter name="os" value="Windows" />
		<parameter name="os_version" value="10" />
		<parameter name="browser" value="Chrome" />
		<parameter name="browser_version" value="85.0" />
		<classes>
			<class name="${ClassName}" />
		</classes>

	</test>

</suite>